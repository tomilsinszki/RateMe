<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        
        {% block stylesheets %}
            <link href="{{ asset('css/global.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        
        {% block javascripts %}
            <script src="{{ asset('js/jquery-1.8.2.min.js') }}" type="text/javascript"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <img src="{{ asset('images/star_empty_150px.png') }}" alt="Empty Star" style="display:none; width:150px;"/>
            <img src="{{ asset('images/star_full_150px.png') }}" alt="Full Star" style="display:none; width:150px;" />
        {% endblock %}
    </body>
</html>
